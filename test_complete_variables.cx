// Complete variable system test
globalString = "I'm a global string!"
globalNumber = 100

function main() {
    localString = "I'm a local string!"
    localNumber = 42
    localBoolean = true
    
    print("=== GLOBAL VARIABLES ===")
    print("Global String:")
    print(globalString)
    print("Global Number:")
    print(globalNumber)
    
    print("=== LOCAL VARIABLES ===")
    print("Local String:")
    print(localString)
    print("Local Number:")
    print(localNumber)
    print("Local Boolean:")
    print(localBoolean)
    
    print("=== VARIABLE SYSTEM COMPLETE! ===")
}
