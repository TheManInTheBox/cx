// Comparison Operations Example - First part
var x = 10
var y = 15
var z = 10

print("Comparison Operations:")
print("x = 10, y = 15, z = 10")

// Equality comparison
var equal1 = x == y    // false
var equal2 = x == z    // true

print("x == y:")
print(equal1)
print("x == z:")
print(equal2)

// Inequality comparison  
var notEqual1 = x != y    // true
var notEqual2 = x != z    // false

print("x != y:")
print(notEqual1)
print("x != z:")
print(notEqual2)
