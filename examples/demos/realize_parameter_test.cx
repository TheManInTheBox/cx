// Test file to verify both realize() patterns work
// Pattern 1: realize() without parameters
conscious SimpleAgent {
    realize() {
        emit agent.initialized {
            message: "Simple agent ready"
        };
    }
    
    on test.event (event) {
        emit response.complete;
    }
}

// Pattern 2: realize(param: object) with object parameter
conscious ParameterizedAgent {
    realize(config: object) {
        emit agent.configured {
            message: "Agent configured with parameters",
            config: config
        };
    }
    
    on configure.event (event) {
        emit configuration.updated {
            status: "complete"
        };
    }
}
