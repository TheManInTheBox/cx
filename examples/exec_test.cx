
/* gets() and string allocation example
 */
#include cxstdio
#include cxctype

int str_to_int(char *str){
    int i=0,s=0;

    while (isdigit(str[i])){
        s = s * 10 + str[i] - '0';
        i++;
    }

    return s;
}

int main () {
    char *i = "12345";

    int a = str_to_int(i);

    return a;
}