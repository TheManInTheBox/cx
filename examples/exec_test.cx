#include io
#include char

namespace bool {
    bool is_false(){		
        return (this == false);
    }
}

namespace char {
    char do_di_do(int a){
        return (this + a);
    }
}

int main () {

    char c = 'F';
    bool a = false;
    bool b = true;
   
    io::puts(c.do_di_do(10).to_str);
	io::puts(a.is_false.to_str);
	io::puts(b.is_false.to_str);

    return 0;
}