int add2or5(int &a){

    if(a > 20){
        a = a + 2;
    } else if(a < 20){
        a = a + 5;
    }else a = 20;

    return a;
}

int pow(int a, int b){
        int c = add2or5(a);
        return a * b;
}

int main(){

    int a = 0;
    int c = 5;

    a = pow(c, c);

    c = a + a;
    
    a = add2or5(c);

    return c;
}