print("🧪 Variable Task Storage Test");

class TaskAgent
{
    name: string;
    
    constructor(agentName)
    {
        this.name = agentName;
        print("🤖 Agent " + this.name + " ready");
    }
    
    function testTaskStorage()
    {
        print("🔍 Testing task variable storage...");
        
        // Store Task object in variable
        var myTask = this.Learn("test_data");
        print("📝 Task stored in variable");
        
        // Try to access the task variable
        print("🔍 About to access task variable...");
        print("Task: " + myTask);
        
        print("✅ Task variable access successful");
        return "task_storage_test_complete";
    }
}

var agent = new TaskAgent("StorageTest");
var result = agent.testTaskStorage();
print("🎯 Result: " + result);
