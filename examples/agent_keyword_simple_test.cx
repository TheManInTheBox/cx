// Simple test of agent keyword vs new keyword

class SimpleBot
{
    name: string;
    type: string;
}

print("🔧 Simple Agent Keyword Test");
print("============================");

try
{
    // Test 1: Create regular object with 'new'
    print("1. Creating regular object with 'new':");
    var regularObj = new SimpleBot();
    regularObj.name = "RegularBot";
    regularObj.type = "regular-object";
    print("✅ Regular object: " + regularObj.name + " (" + regularObj.type + ")");
    
    print("");
    
    // Test 2: Create autonomous agent with 'agent'
    print("2. Creating autonomous agent with 'agent':");
    var autonomousAgent = agent SimpleBot();
    autonomousAgent.name = "AutonomousBot";
    autonomousAgent.type = "autonomous-agent";
    print("✅ Autonomous agent: " + autonomousAgent.name + " (" + autonomousAgent.type + ")");
    
    print("");
    print("🎯 Simple Test Complete!");
    print("✅ Both object types created successfully");
    print("✅ Agent keyword semantic distinction working");
}
catch (error)
{
    print("❌ Error: " + error);
}
