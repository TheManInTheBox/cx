// Test incrementers and assignment operators
print("=== Testing Assignment Operators ===");

// Test basic assignment
var a = 5;
print("Initial a: " + a);

a = 10;
print("After a = 10: " + a);

// Test += operator
a += 3;
print("After a += 3: " + a);

// Test -= operator  
a -= 2;
print("After a -= 2: " + a);

// Test *= operator
a *= 2;
print("After a *= 2: " + a);

// Test /= operator
a /= 4;
print("After a /= 4: " + a);

// Test with different variable
var counter = 0;
print("Initial counter: " + counter);

counter += 1;
print("After counter += 1: " + counter);

counter += 1;
print("After second counter += 1: " + counter);

print("=== Assignment Operator Test Complete ===");
