class NoServiceTest
{
    name: string;
    
    constructor(testName)
    {
        this.name = testName;
        print("No service test created: " + this.name);
    }
    
    function simpleTest()
    {
        print("Simple test method called");
    }
}

try
{
    print("Testing class without service injection...");
    var test = new NoServiceTest("NO_SERVICE_TEST");
    test.simpleTest();
    print("✅ SUCCESS: Basic class creation working!");
}
catch (error)
{
    print("❌ ERROR: " + error);
}
