// Test async service method calls with visible results
class AsyncServiceTest
{
    uses textGen from Cx.AI.TextGeneration;
    
    function testAsyncCall()
    {
        print("Testing async GenerateAsync call...");
        
        var result = this.textGen.GenerateAsync("Say 'Hello from AI!' in exactly 3 words.");
        print("Result: " + result);
        
        return result;
    }
}

var test = new AsyncServiceTest();
test.testAsyncCall();
