function main() {
    var x = 42
    var y = "Hello, World!"
    var z = "Another variable"
    
    print("x =")
    print(x)
    print("y =")
    print(y)
    print("z =")
    print(z)
    
    // Test reassignment as expression statement
    x = 100
    print("x after reassignment =")
    print(x)
}
