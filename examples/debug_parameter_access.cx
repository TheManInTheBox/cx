// Test parameter access in constructor

class ParameterTest
{
    name: string;
    
    constructor(inputName)
    {
        print("About to assign parameter to field");
        this.name = inputName;  // This works
        print("About to print parameter directly");
        print(inputName);       // This might fail
        print("Constructor complete");
    }
}

print("=== Creating instance ===");
var obj = new ParameterTest("TestValue");
print("Success!");
