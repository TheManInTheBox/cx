// Test class method without field access

print("=== Simple Method Test ===");

class TestClass
{
    constructor()
    {
        print("Constructor called");
    }
    
    function sayHello()
    {
        print("Hello from method!");
    }
}

print("Creating TestClass...");
var obj = new TestClass();

print("Calling sayHello...");
obj.sayHello();

print("=== Test Complete ===");
