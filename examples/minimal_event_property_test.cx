// Minimal test to verify event parameter property access works  
class EventTestAgent
{
    on user.input (event)
    {
        print("Message: " + event.payload);

        for (var eve in event.payload)
        {
            print(eve.Key);
            print(eve.Value);
        }
    }
}

var agent = new EventTestAgent();
emit user.input { message: "Hello", type: "test" };
