// Test service injection with minimal complexity

class ServiceTest
{
    uses textGen from Cx.AI.TextGeneration;
    name: string;
    
    constructor(testName)
    {
        this.name = testName;
        print("ServiceTest constructor called");
    }
    
    function test()
    {
        print("Test method called on: " + this.name);
    }
}

try
{
    print("Creating service test instance...");
    var test = new ServiceTest("SimpleService");
    test.test();
    print("✅ Service test completed successfully!");
}
catch (error)
{
    print("❌ ERROR: " + error);
}
