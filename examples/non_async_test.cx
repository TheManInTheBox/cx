// Test non-async inheritance methods
print("🧠 Testing Non-Async Inheritance Methods");

class SimpleAgent
{
    function simpleTest(input)
    {
        print("📝 Processing input: " + input);
        return "Processed: " + input;
    }
}

print("\n🚀 Creating agent...");
var agent = new SimpleAgent();
var result = agent.simpleTest("test data");
print("✅ Result: " + result);

print("\n🎉 Non-async test complete!");
