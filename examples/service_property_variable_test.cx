class TestClass
{
    uses textGen from Cx.AI.TextGeneration;

    function testServicePropertyDirect()
    {
        // Test different ways to access the property
        print("Test 1: Variable assignment");
        var service = this.textGen;
        print("Got service: " + service);
        
        print("Test 2: Try property access on variable"); 
        // This should NOT crash if the issue is with direct field.property access
        var name = service.ServiceName;
        print("Service name from variable: " + name);
    }
}

var test = new TestClass();
test.testServicePropertyDirect();
