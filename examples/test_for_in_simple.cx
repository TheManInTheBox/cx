// Advanced for-in loop tests (no string concatenation)
var fruits = ["apple", "banana", "cherry"];
var numbers = [10, 20, 30];

print("=== Fruit Iteration ===");
for (fruit in fruits) 
{
    print(fruit);
}

print("=== Number Processing ===");
var total = 0;
for (num in numbers) 
{
    total += num;
    print(total);
}

print("=== Final total ===");
print(total);

print("=== Empty Array Test ===");
var empty = [];
for (item in empty) 
{
    print("This should not print");
}
print("Empty array iteration completed");

print("=== Mixed Types ===");
var mixed = [42, "hello", 99];
for (item in mixed) 
{
    print(item);
}
