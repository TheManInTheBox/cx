/* Array initialization examples
 */
#include cxmath

int main () {

    // array initialization list
    int *b = {
              4096,
              4096 * 2,
              4096 * 3,
              4096 * 4,
              4096 * 5
    };

    //dynamic array container
    int *sqr;

    int i = 0;
    do {
        // allocate new int at the end of array
        sqr += isqrt(b[i]);
    } while (i++ < 5);

    return 0;
}
