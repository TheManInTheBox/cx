// Test enhanced grammar features
function fibonacci(n) 
{
    if (n <= 1) 
    {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

// Test try-catch
try 
{
    var result = fibonacci(5);
} 
catch (error) 
{
    throw new Error("Failed to calculate");
}

