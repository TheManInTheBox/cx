
/* standard type member functions example
 * 
 * var.to_str() - returns the string copy of the valuw of var
 */

#include cxstdio

int main () {

	char *s = "some text";
	char *a = s + s + '!';

	// s[3] == 'e'
	puts("the length of s[3] is " + s[3].to_str.length.to_str);
	puts(a + " string length is " + a.length.to_str + " characters!");

    return 0;
}