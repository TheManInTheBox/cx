class SimpleServiceTest
{
    uses textGen from Cx.AI.TextGeneration;
    
    name: string;
    
    constructor(testName)
    {
        this.name = testName;
        print("Simple service test created: " + this.name);
    }
    
    function simpleTest()
    {
        print("Simple test method called");
    }
}

try
{
    print("Testing simple service injection...");
    var serviceTest = new SimpleServiceTest("SIMPLE_TEST");
    serviceTest.simpleTest();
    print("✅ SUCCESS: Simple service injection working!");
}
catch (error)
{
    print("❌ ERROR: " + error);
}
