// Test basic if statement with semicolons

var x = 10;
var y = 5;

print("Testing if statement:");

if (x > y) {
    print("x is greater than y");
}

print("Testing if-else:");

if (x < y) {
    print("x is less than y");
} else {
    print("x is NOT less than y");
}

print("If statement tests complete");
