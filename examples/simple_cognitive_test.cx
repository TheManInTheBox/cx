class SimpleAgent
{
    name: string;
    
    constructor(agentName)
    {
        this.name = agentName;
        print("Created simple agent: " + this.name);
    }
    
    function simpleTest()
    {
        return "Simple test result: " + this.name;
    }
}

print("Creating simple agent...");
var simple = new SimpleAgent("TestAgent");

print("Testing basic method...");
var result = simple.simpleTest();
print("Basic method result: " + result);

