// Advanced for-in loop tests
var fruits = ["apple", "banana", "cherry"];
var numbers = [10, 20, 30];

print("=== Fruit Iteration ===");
for (fruit in fruits) {
    print("Fruit: " + fruit);
}

print("\n=== Number Processing ===");
var total = 0;
for (num in numbers) {
    total += num;
    print("Adding " + num + ", total now: " + total);
}

print("\n=== Final total: " + total);

print("\n=== Empty Array Test ===");
var empty = [];
for (item in empty) {
    print("This should not print");
}
print("Empty array iteration completed");

print("\n=== Nested Loop Test ===");
var matrix = [[1, 2], [3, 4]];
for (row in matrix) {
    print("Processing row...");
    for (cell in row) {
        print("  Cell value: " + cell);
    }
}
