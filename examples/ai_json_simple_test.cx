// Simple test of AI functions returning native CX objects from JSON responses
// This demonstrates basic AI function calls without typeof

var message = "Testing AI functions with JSON object returns";
print(message);

print("=== AI Functions Test ===");

// Test task function - should return a CX object if response contains JSON
var taskResult = task("Generate a simple JSON response with a name and age");
print("Task Result: " + taskResult);

// Test reason function
var reasonResult = reason("Analyze this simple problem");
print("Reason Result: " + reasonResult);

// Test generate function
var generateResult = generate("Create a simple data structure");
print("Generate Result: " + generateResult);

// Test synthesize function
var synthesizeResult = synthesize("Combine these concepts: data and structure");
print("Synthesize Result: " + synthesizeResult);

// Test process function
var processResult = process("input data", "transform to output");
print("Process Result: " + processResult);

// Test embed function
var embedResult = embed("This is text to embed");
print("Embed Result: " + embedResult);

// Test adapt function
var adaptResult = adapt("some content to adapt");
print("Adapt Result: " + adaptResult);

print("=== AI Functions Test Complete ===");
