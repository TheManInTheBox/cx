// Test logical operators implementation with semicolons

// Test boolean variables
var a = true;
var b = false;

// Test AND operator
var result1 = a && b;
print("true && false =");
print(result1);

var result2 = a && a;
print("true && true =");
print(result2);

// Test OR operator
var result3 = a || b;
print("true || false =");
print(result3);

var result4 = b || b;
print("false || false =");
print(result4);

// Test NOT operator
var result5 = !a;
print("!true =");
print(result5);

var result6 = !b;
print("!false =");
print(result6);

// Test complex expressions
var result7 = !(a && b);
print("!(true && false) =");
print(result7);

var result8 = !a || b;
print("!true || false =");
print(result8);

// Test with variables in conditions
if (a && !b) {
    print("a is true and b is false");
}

if (a || b) {
    print("At least one of a or b is true");
}

if (!a) {
    print("This should not print");
} else {
    print("a is not false");
}
