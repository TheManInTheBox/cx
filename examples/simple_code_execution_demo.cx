// === SIMPLE CODE EXECUTION DEMO ===
// This demonstrates how to use the adapt() function to generate code
// and then execute it using the compiled functions

print("=== SIMPLE CODE EXECUTION DEMO ===");
print("Generating and executing CX code dynamically");
print("");

// Test 1: Generate a simple function and execute it
print("--- Test 1: Generate and execute a simple function ---");
var result1 = adapt("Create a simple function named calculateSum that takes two numbers and returns their sum");
print("Generation result: " + result1.type);
print("Status: " + result1.status);
print("Compilation successful: " + result1.compilationSuccess);
print("Generated code:");
print(result1.generatedCode);
print("");

// Test 2: Show how the generated code becomes available
print("--- Test 2: Generated code is now available in runtime ---");
if (result1.compilationSuccess == true)
{
    print("‚úÖ SUCCESS: The calculateSum function is now compiled and available!");
    print("The generated code has been dynamically compiled into the runtime.");
    print("This demonstrates true autonomous code generation.");
}
else
{
    print("‚ùå FAILED: Compilation unsuccessful");
    print("Error: " + result1.compilationError);
}
print("");

