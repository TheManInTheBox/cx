// Test logical operators in tests directory

var a = true
var b = false
var x = 10
var y = 5

print("=== Testing Logical Operators ===")

// Test AND
print("Testing AND:")
var and1 = a && b
print(and1)

var and2 = a && a  
print(and2)

// Test OR
print("Testing OR:")
var or1 = a || b
print(or1)

var or2 = b || b
print(or2)

// Test NOT
print("Testing NOT:")
var not1 = !a
print(not1)

var not2 = !b
print(not2)

// Test with comparisons
print("Testing with comparisons:")
if (x > y && a)
{
    print("x > y AND a is true")
}

if (x < y || a)
{
    print("x < y OR a is true")
}

if (!(x < y))
{
    print("NOT (x < y)")
}
